<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFrameworks>netstandard2.0;netstandard2.1</TargetFrameworks>
        <LangVersion>latest</LangVersion>
        <Nullable>enable</Nullable>
        <Version>1.0.1</Version>
        <Authors>Olivier Coanet</Authors>
        <Description>A decoder for unknown protocol buffer encoded data.</Description>
        <Copyright>Olivier Coanet</Copyright>
        <PackageProjectUrl>https://github.com/ocoanet/ProtoUntyped</PackageProjectUrl>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageTags>binary;serialization;protobuf</PackageTags>
        <PackageOutputPath>..\..\output</PackageOutputPath>
        <PublishRepositoryUrl>true</PublishRepositoryUrl>
        <EmbedUntrackedSources>true</EmbedUntrackedSources>
        <IncludeSymbols>true</IncludeSymbols>
        <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0" PrivateAssets="All" />
        <PackageReference Include="protobuf-net" Version="3.0.73" />
    </ItemGroup>

</Project>
